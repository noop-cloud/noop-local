{
  "name": "noop-local",
  "bin": {
    "noop": "./cli.js"
  },
  "version": "1.0.43",
  "description": "Noop Local Development Environment",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noop-cloud/noop-local.git"
  },
  "keywords": [
    "noop",
    "noop-local",
    "noop-cloud",
    "noopfile",
    "rearc"
  ],
  "author": "Chloe Wintzer",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/noop-cloud/noop-local/issues"
  },
  "scripts": {
    "postversion": "git push && git push --tags",
    "install-console": "npm install --prefix lib/inspector/console",
    "update-console": "npm update --prefix lib/inspector/console",
    "build-console": "NODE_ENV=production npm run build --prefix lib/inspector/console",
    "ci-console": "npm ci --prefix lib/inspector/console",
    "dev-console": "npm run serve --prefix lib/inspector/console"
  },
  "homepage": "https://github.com/noop-cloud/noop-local#readme",
  "dependencies": {
    "async": "^2.6.1",
    "chalk": "^2.4.1",
    "chokidar": "^3.5.1",
    "cors": "^2.8.5",
    "dockerode": "^2.5.7",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "ignore": "^5.1.8",
    "jsonpath": "^1.1.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.29.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "noop-discovery": "^1.3.18",
    "path-to-regexp": "^2.4.0",
    "rimraf": "^2.6.2",
    "tar-fs": "^1.16.3",
    "tar-stream": "^1.6.2",
    "ws": "^7.4.2",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6"
  }
}
